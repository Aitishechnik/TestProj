@model TestProj.Models.Coin

@{
    ViewData["Title"] = "EditCoin";
}

<h3>Edit Coin</h3>
<form asp-action="AddCoin" method="post">
    <label asp-for="Country"></label>
    <input class="form-control" asp-for="Country" />
    <label asp-for="Metal"></label>
    <input class="form-control" asp-for=Metal />
    <label asp-for="Denomination"></label>
    <input class="form-control" asp-for="Denomination" />
    <button type="submit" asp-controller="Home" asp-action="Save" asp-route-id="@Model?.Id" class="btn btn-primary mt-2">OK</button>
</form>
